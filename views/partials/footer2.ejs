<script>
    const menu_icon = document.querySelector('.menu-icon');
    function addClassFunThree() {
        this.classList.toggle("click-menu-icon");
    }
    menu_icon.addEventListener('click', addClassFunThree);

    // Custom Code 

     function validcompSetting(form){
        
        var date=form.dateOrigin.value;
     var datemsg=document.getElementById('dateOrigin');
    var eighteenYearsAgo = moment().subtract(1, "years");
    var birthday = moment(date);

    if (!birthday.isValid()) {
        datemsg.innerText="Enter Valid Date";
        form.dateOrigin.focus();
        return false;    
    }
    else if (eighteenYearsAgo.isAfter(birthday)) {
        return true;    
    }
    else {
        return false;   
        datemsg.innerText="Enter Valid Date"; 
    }
   
}

function validUserFormSetting(form){
    var date=form.birthDate.value;
     var datemsg=document.getElementById('validDate');
    var eighteenYearsAgo = moment().subtract(1, "years");
    var birthday = moment(date);

    if (!birthday.isValid()) {
        datemsg.innerText="Enter Valid Date";
        form.birthDate.focus();
        return false;    
    }
    else if (eighteenYearsAgo.isAfter(birthday)) {
        return true;    
    }
    else {
         
        datemsg.innerText="Enter Valid Date"; 
        return false;
    }
}

