		//Check password and confirm password
		function checkForm(form)
 	 {
		  var errMsg=document.getElementById("error-nwl");
		  var countryError=document.getElementById("countryError");
		  var messagefirst = document.getElementById('firstNameError');
		  var messagelast = document.getElementById("lastNameError");
		  var messageemail = document.getElementById("emailError");
    if(form.password.value != "" && form.password.value == form.confirmPassword.value) {
      if(form.password.value.length < 8) {
        alert("Error: Password must contain at least 8 characters!");
        form.password.focus();
        return false;
      }
    
      re = /^(?=.*[A-Za-z])(?=.*\d)(?=.*[@$.!%*#?&])[A-Za-z\d@$.!%*#?&]{8,}$/;
      if(!re.test(form.password.value)) {
		errMsg.innerText="Error: password must contain at least one number (0-9) and one capital character and one symbol and small character!";
		form.password.focus();
        return false;
      }
   
	}
	 else {
	  errMsg.innerText="Error: Please check that you've entered and confirmed your password correctly!";
      form.password.focus();
      return false;
	}
	if(form.country.value=="-- Select Country --"){
		countryError.innerText="Country must be provided";
		return false;
	}
	if(form.firstName.value==""){
		
		messagefirst.innerText="First Name Must  be  provided";
		return false;
	
	}
	if(form.lastName.value==""){
		
		messagelast.innerText="Last Name Must  be  provided";
		return false;
	}
	if(form.email.value==""){
		
		messageemail.innerText="Last Name Must  be  provided";
		return false;
	}

  
    return true;
  }

